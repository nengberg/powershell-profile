{
	"plugins": [ "git", "hg", "ssh", "z", "aliases" ],
	"prompt": [
		[ "darkblue",  "", "$path" ],
		[ "darkcyan",   "", " $git_lbracket$git_branch$git_dirty$git_rbracket" ],
		[ "darkcyan",   "", " $hg_lbracket$hg_branch$hg_dirty$hg_rbracket" ],
		[ "darkblue", "", " `n$" ]
	],
	"git": {
		"prompt_dirty": "*",
		"prompt_lbracket": "(",
		"prompt_rbracket": ")"
	},
	"hg": {
		"prompt_dirty": "*",
		"prompt_lbracket": "(",
		"prompt_rbracket": ")"
	}
}